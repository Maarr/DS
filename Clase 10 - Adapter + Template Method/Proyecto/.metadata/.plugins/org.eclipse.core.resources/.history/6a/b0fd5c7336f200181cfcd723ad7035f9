package game;

import adapters.Platform;
import platform.Image2D;

public abstract class Game {
	
	Platform platform;
	Image2D image;
	
	public Game(Platform p) {
		this.platform=p;
	}
	
	public void launch(String file) {
		image=platform.loadImage(file);
		
		
	}
	
	abstract void play(Image2D);

}

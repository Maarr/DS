package modificaciones;

import java.io.FileReader;
import java.io.FileWriter;
import java.io.IOException;

import fileSystem.FileSystem;

public class NormalizarSaltosLinea {
	
	private FileSystem fs;
	
	public NormalizarSaltosLinea(FileSystem fs) {
		this.fs=fs;
	}
	
	public void normalizarSaltos() {
		try {
			FileWriter fw=new FileWriter(f);

			int c;
			while ((c = reader.read()) != -1)
				output.send((char) c);

			reader.close();
			output.close();

		} catch (IOException e) {
			e.printStackTrace();
		}
	}

}

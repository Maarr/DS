package cambios;

import editor.Dibujo;
import editor.Figura;

public class CambioMover implements Cambio{
	
	private Figura figuraAntes;
	private Figura figuraDespues;
	private Dibujo dibujo;
	
	public CambioMover(Figura antes,Figura despues, Dibujo dibujo) {
		this.figuraAntes=antes;
		this.figuraDespues=despues;
		this.dibujo=dibujo;
	}

	@Override
	public void undo() {
		dibujo.
		
	}

	@Override
	public void redo() {
		// TODO Auto-generated method stub
		
	}

}

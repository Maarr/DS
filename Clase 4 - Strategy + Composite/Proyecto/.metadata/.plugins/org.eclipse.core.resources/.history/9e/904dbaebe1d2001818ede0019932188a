package datos;

import formulario.Campo;
import validadores.Validador;

public class Edad extends DatoFormulario{
	
	public Edad() {
		super(new Campo("Edad"));
	}

	@Override
	public boolean validar() {
		Validador v=super.getValidador();
		return (v.getNumeros().validarSoloDigitos(super.getDato()) && v.getMayorQue().validarMayorQue(super.getDato(), 18));
	}

}

package datos;

import campos.Campo;

public abstract class Dato {
	
	private Campo campo;
	private String valor;
	
	public Dato(Campo campo) {
		this.campo=campo;
	}
	
	public void pideDato() {
		this.valor=campo.pideDato();
	}
	
	public String getDato() {
		return valor;
	}
	
	public abstract boolean validar();
	
	

}

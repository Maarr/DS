package datos;

import campos.Campo;

public class Edad extends DatoFormulario{
	
	public Edad() {
		super(new Campo("Edad"));
	}

	@Override
	public boolean validar() {
		return (super.getValidador().validarSoloDigitos(super.getDato()) && super.getValidador().validarMayorQue(super.getDato(), 18));
	}

}

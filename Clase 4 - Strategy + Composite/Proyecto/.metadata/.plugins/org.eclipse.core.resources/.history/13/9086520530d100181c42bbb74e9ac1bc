package datos;

import campos.Campo;

public class Ubicacion extends DatoFormulario{
	
	public Ubicacion() {
		super(new Campo("Ubicación"));
	}

	@Override
	public boolean validar() {
		String[] valores= {"Santander", "Oviedo", "Cádiz"};
		return ((super.getValidador().validarLongitud(super.getDato(), 4) && super.getValidador().validarSoloDigitos(super.getDato())) ||
				(super.getValidador().validarCumpleValores(super.getDato(), valores)));
	}

}
